docker build -t gcc .
docker run -it --rm --name fib --cpus="1" -m 256m gcc

docker run -it --name --restart=unless-stopped fib gcc
docker stop fib
docker ps -a
docker restart fib
docker ps -a

docker stop fib
docker ps -a

docker rm fib
docker rmi gcc

docker run -it --rm -v "$PWD":/usr/src/myapp -w /usr/src/myapp gcc:4.9 gcc -o output main.c